{
  "$schema": "https://turborepo.com/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "env": ["S3_*", "GOOGLE_*", "DATABASE_URL", "AUTH_SECRET"],
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**", "dist/**"]
    },
    "build:ci": {
      "env": ["S3_*", "GOOGLE_*", "DATABASE_URL", "AUTH_SECRET"],
      "dependsOn": ["^build"],
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": [".next/**", "!.next/cache/**", "dist/**"]
    },
    "lint": {
      "dependsOn": ["^lint"]
    },
    "check-types": {
      "dependsOn": ["^check-types"]
    },
    "test": {
      
    },
    "start": {
      
    },
    "proxy-ssl": {
      
    },
    "dev": {
      "dependsOn": ["^build"],
      "cache": false,
      "persistent": true
    },
    "dev:https": {
      "dependsOn": ["^build"],
      "cache": false,
      "persistent": true
    }
  }
}
