{
  "name": "territorios-multitenant",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "build:ci": "turbo run build:ci",
    "dev": "turbo run dev",
    "dev:https": "turbo run dev:https",
    "start": "turbo run start",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "proxy-ssl": "turbo run proxy-ssl"
  },
  "devDependencies": {
    "local-ssl-proxy": "^2.0.5",
    "prettier": "^3.5.3",
    "turbo": "^2.6.1",
    "typescript": "5.9.3"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@10.9.2",
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
