{
  "name": "api",
  "version": "0.0.1",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "build:ci": "prisma generate --sql && nest build",
    "start": "nest start -b swc",
    "dev": "nest start --watch -b swc",
    "dev:https": "cross-env HTTPS=true nest start --watch -b swc",
    "debug": "nest start --debug --watch",
    "preview": "node dist/src/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --max-warnings 0",
    "check-types": "tsc --noEmit",
    "pretest": "prisma generate",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "db:run": "docker run --name territorios-pg -e POSTGRES_PASSWORD=secret -e POSTGRES_DB=application -p 5432:5432 postgres",
    "db:start": "docker start learn-with-ai-pg -ia",
    "create-cert": "mkcert -install && mkcert -cert-file certificates/localhost.pem -key-file certificates/localhost-key.pem localhost *.territorios.app territorios.app",
    "proxy-ssl": "local-ssl-proxy --source 8443 --target 3333 --key certificates/localhost-key.pem --cert certificates/localhost.pem"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.812.0",
    "@nestjs/common": "^11.1.1",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.1",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.1",
    "@prisma/adapter-pg": "^7.0.1",
    "@prisma/client": "^7.0.1",
    "@repo/utils": "*",
    "bcrypt": "^5.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^16.0.3",
    "express-session": "^1.18.1",
    "lodash-es": "^4.17.21",
    "multer-s3": "^3.0.1",
    "parse-duration": "^2.1.4",
    "passport": "^0.7.0",
    "passport-custom": "^1.1.1",
    "passport-google-oauth20": "^2.0.0",
    "passport-google-oidc": "^0.1.0",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "uuid": "^11.1.0",
    "zod": "^4.0.14"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.7",
    "@nestjs/schematics": "^11.0.5",
    "@nestjs/testing": "^11.1.1",
    "@repo/eslint-config": "*",
    "@repo/typescript-config": "*",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^5.0.1",
    "@types/express-session": "^1.18.1",
    "@types/jest": "^29.5.14",
    "@types/lodash-es": "^4.17.12",
    "@types/multer": "^1.4.12",
    "@types/multer-s3": "^3.0.3",
    "@types/node": "^22.10.7",
    "@types/passport": "^1.0.17",
    "@types/passport-google-oauth20": "^2.0.16",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.2",
    "cross-env": "^10.1.0",
    "eslint": "^9.18.0",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "prisma": "^7.0.1",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.3.2",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "~"
  }
}
