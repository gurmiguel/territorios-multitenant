{
  "$schema": "https://turborepo.com/schema.json",
  "extends": ["//"],
  "tasks": {
    "build": {
      "passThroughEnv": ["S3_*", "GOOGLE_*", "DATABASE_URL", "AUTH_SECRET"]
    },
    "build:ci": {
      "env": ["DATABASE_URL"],
      "passThroughEnv": ["S3_*", "GOOGLE_*", "AUTH_SECRET"]
    }
  }
}